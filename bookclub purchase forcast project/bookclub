   "source": [
    "#Q9\n",
    "# Constants\n",
    "cost_per_mail = 2        # Cost of promotional brochure and mailing\n",
    "cost_per_book = 20       # Cost to procure and mail each book\n",
    "selling_price = 42       # Selling price of the book\n",
    "\n",
    "# Number of customers in the validation sample\n",
    "total_customers = len(bookclub_validation)\n",
    "\n",
    "# Number of customers who made a purchase\n",
    "num_purchases = bookclub_validation['Choice'].sum()\n",
    "\n",
    "# Calculating total revenue, costs, and net profits for blanket promotion strategy\n",
    "total_revenue = num_purchases * selling_price\n",
    "total_mailing_cost = total_customers * cost_per_mail\n",
    "total_book_cost = num_purchases * cost_per_book\n",
    "total_costs = total_mailing_cost + total_book_cost\n",
    "net_profit = total_revenue - total_costs\n",
    "\n",
    "# Displaying the results\n",
    "print(\"Number of Customers Who Made a Purchase:\", num_purchases)\n",
    "print(\"Total Revenue: $\", total_revenue)\n",
    "print(\"Total Costs: $\", total_costs)\n",
    "print(\"Net Profit: $\", net_profit)\n"
   ]
  },
