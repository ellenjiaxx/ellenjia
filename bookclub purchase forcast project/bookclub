   "source": [
    "#Q1\n",
    "import pandas as pd\n",
    "import statsmodels.api as sm\n",
    "\n",
    "# Load the training and validation datasets\n",
    "training_file_path = '/Users/ellenjia/Desktop/bookclub_training.csv'  # Replace with your actual file path\n",
    "validation_file_path = '/Users/ellenjia/Desktop/bookclub_validation.csv'  # Replace with your actual file path\n",
    "\n",
    "bookclub_training = pd.read_csv(training_file_path)\n",
    "bookclub_validation = pd.read_csv(validation_file_path)\n",
    "\n",
    "# Calculate the proportion of customers who made a purchase\n",
    "proportion_purchases = bookclub_training['Choice'].mean()\n",
    "print(f\"Proportion of customers who made a purchase: {proportion_purchases:.2f}\")\n",
    "\n",
    "# Prepare the independent variables (excluding 'Choice' and 'customer')\n",
    "X = bookclub_training.drop(['Choice', 'customer'], axis=1)\n",
    "y = bookclub_training['Choice']\n",
    "\n",
    "# Adding a constant to the model (intercept)\n",
    "X = sm.add_constant(X)\n",
    "\n",
    "# Building the logistic regression model\n",
    "model = sm.Logit(y, X)\n",
    "result = model.fit()\n",
    "\n",
    "# Display the model summary\n",
    "print(result.summary())\n",
    "\n"
   ]
  },
