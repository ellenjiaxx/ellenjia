   "source": [
    "#Q8\n",
    "import pandas as pd\n",
    "\n",
    "training_file_path = '/Users/ellenjia/Desktop/bookclub_training.csv'\n",
    "validation_file_path = '/Users/ellenjia/Desktop/bookclub_validation.csv'\n",
    "\n",
    "training_data = pd.read_csv(training_file_path)\n",
    "validation_data = pd.read_csv(validation_file_path)\n",
    "\n",
    "X_train = training_data.drop(['customer', 'Choice'], axis=1)\n",
    "y_train = training_data['Choice']\n",
    "X_validation = validation_data.drop(['customer', 'Choice'], axis=1)\n",
    "y_validation = validation_data['Choice']\n",
    "\n",
    "from sklearn.ensemble import RandomForestClassifier\n",
    "\n",
    "rf_clf = RandomForestClassifier(n_estimators=30, max_depth=4, random_state=603, oob_score=True)\n",
    "rf_clf.fit(X_train, y_train)\n",
    "\n",
    "y_validation_pred_rf = rf_clf.predict(X_validation)\n",
    "from sklearn.metrics import confusion_matrix, classification_report\n",
    "\n",
    "conf_matrix_validation_rf = confusion_matrix(y_validation, y_validation_pred_rf)\n",
    "accuracy_validation_rf = (conf_matrix_validation_rf[0, 0] + conf_matrix_validation_rf[1, 1]) / conf_matrix_validation_rf.sum()\n",
    "class_report_validation_rf = classification_report(y_validation, y_validation_pred_rf)\n",
    "# Print the Confusion Matrix\n",
    "print(\"Confusion Matrix:\")\n",
    "print(conf_matrix_validation_rf)\n",
    "\n",
    "# Print the Accuracy\n",
    "accuracy_validation_rf = (conf_matrix_validation_rf[0, 0] + conf_matrix_validation_rf[1, 1]) / conf_matrix_validation_rf.sum()\n",
    "print(\"\\nAccuracy:\", accuracy_validation_rf)\n",
    "\n",
    "# Print the Classification Report\n",
    "class_report_validation_rf = classification_report(y_validation, y_validation_pred_rf)\n",
    "print(\"\\nClassification Report:\")\n",
    "print(class_report_validation_rf)"
   ]
  },
