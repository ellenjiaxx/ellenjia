    "from sklearn.metrics import confusion_matrix, accuracy_score, classification_report\n",
    "\n",
    "# Predicting the outcomes on the training data\n",
    "y_pred = result.predict(X) > 0.5  # Using 0.5 as the cutoff for classification\n",
    "\n",
    "# Generating the confusion matrix and calculating the accuracy\n",
    "conf_matrix = confusion_matrix(y, y_pred)\n",
    "accuracy = accuracy_score(y, y_pred)\n",
    "\n",
    "# Displaying the confusion matrix and accuracy\n",
