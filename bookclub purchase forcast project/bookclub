   ],
   "source": [
    "#Q10\n",
    "# Predicting purchase probabilities for each customer in the validation sample\n",
    "purchase_probabilities = result.predict(X_val)\n",
    "\n",
    "# Adding predicted probabilities to the validation data\n",
    "bookclub_validation['predicted_prob'] = purchase_probabilities\n",
    "\n",
    "# Selecting top 50% customers based on predicted purchase probability\n",
    "top_50_percent = bookclub_validation.sort_values(by='predicted_prob', ascending=False).head(total_customers // 2)\n",
    "\n",
    "# Number of actual purchases within the top 50% targeted customers\n",
    "actual_purchases_top_50 = top_50_percent['Choice'].sum()\n",
    "\n",
    "# Calculating total revenue, costs, and net profits for the targeted promotion strategy\n",
    "total_revenue_targeted = actual_purchases_top_50 * selling_price\n",
    "total_mailing_cost_targeted = (total_customers // 2) * cost_per_mail\n",
    "total_book_cost_targeted = actual_purchases_top_50 * cost_per_book\n",
    "total_costs_targeted = total_mailing_cost_targeted + total_book_cost_targeted\n",
    "net_profit_targeted = total_revenue_targeted - total_costs_targeted\n",
    "\n",
    "# Displaying the results for the targeted strategy\n",
    "print(\"Actual Purchases in Targeted Top 50%:\", actual_purchases_top_50)\n",
    "print(\"Total Revenue from Targeted Strategy: $\", total_revenue_targeted)\n",
    "print(\"Total Costs from Targeted Strategy: $\", total_costs_targeted)\n",
    "print(\"Net Profit from Targeted Strategy: $\", net_profit_targeted)\n"
   ]
