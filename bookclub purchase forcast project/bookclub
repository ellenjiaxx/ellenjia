    "#Q6\n",
    "from sklearn.tree import DecisionTreeClassifier, plot_tree\n",
    "\n",
    "# Assuming you have already built the tree_classifier with max_depth=2 and random_state=603\n",
    "# And that your validation data (X_val and y_val) is already prepared\n",
    "\n",
    "# Predicting for the first observation in the validation sample\n",
    "first_observation = X_val.iloc[0:1]\n",
    "prediction = tree_classifier.predict(first_observation)\n",
    "\n",
    "# Finding the node where the first observation ends up\n",
    "node_indicator = tree_classifier.decision_path(first_observation)\n",
    "leaf_id = node_indicator.indices[node_indicator.indptr[1]-1]  # Getting the leaf node id\n",
    "\n",
    "# Displaying the prediction and leaf node id\n",
    "print(\"Prediction for the first observation:\", 'Purchase' if prediction[0] == 1 else 'Non-purchase')\n",
    "print(\"Leaf node where the first observation ends up:\", leaf_id)\n"
   ]
