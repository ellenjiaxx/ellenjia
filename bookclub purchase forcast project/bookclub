    "#Q7\n",
    "from sklearn.ensemble import RandomForestClassifier\n",
    "\n",
    "# Building a random forest model\n",
    "rf_classifier = RandomForestClassifier(n_estimators=30, max_depth=4, random_state=603, oob_score=True)\n",
    "rf_classifier.fit(X, y)\n",
    "\n",
    "# OOB (Out-of-Bag) accuracy\n",
    "oob_accuracy = rf_classifier.oob_score_\n",
    "\n",
    "# Feature importances\n",
    "feature_importances = rf_classifier.feature_importances_\n",
    "features = X.columns\n",
    "feature_importance_dict = dict(zip(features, feature_importances))\n",
    "\n",
    "# Sorting features by importance and getting the top three\n",
    "top_three_features = sorted(feature_importance_dict, key=feature_importance_dict.get, reverse=True)[:3]\n",
    "\n",
    "# Displaying OOB accuracy and top three most important variables\n",
    "print(\"OOB Accuracy:\", oob_accuracy)\n",
    "print(\"Top three most important variables:\", top_three_features)\n"
   ]
